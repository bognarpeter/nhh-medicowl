<header>
  <nav>
    <div class="nav-wrapper">
      <a href="#!" class="brand-logo center">MedicOwl</a>
      <ul class="left hide-on-med-and-down">
        <li><a href="/overview">Diagnosis</a></li>
        <li><a href="/history">History</a></li>
        <li><a href="/contacts">Contacts</a></li>
        <li><a href="#" onclick="openModal('settings-modal')">Settings</a></li>
      </ul>
    </div>
  </nav>
  <div id="settings-modal" class="modal">
    <div class="modal-content">
      <div class="row">
        <form class="col s12">
          <div class="row">
            <div class="input-field col s6">
              <input id="first_name" type="text" class="validate">
              <label for="first_name">First Name</label>
            </div>
            <div class="input-field col s6">
              <input id="last_name" type="text" class="validate">
              <label for="last_name">Last Name</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <label for="date_birth">Date of Birth (dd/mm/yyyy)</label>
              <input id="date_birth" type="text" class="validate">
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <label for="address">Address</label>
              <input id="address" type="text" class="validate">
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <input id="mobile_number" type="text" class="validate">
              <label for="mobile_number">Telephone Number</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <input id="email" type="text" class="validate">
              <label for="email">Email</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <input id="health_number" type="text" class="validate">
              <label for="health_number">Helath Insurance Number</label>
            </div>
          </div>

        </form>
      </div>
      <div class="modal-footer">
        <a href="#!" class="modal-close waves-effect waves-green btn-flat">Ok</a>
      </div>
    </div>
  </div>
</header>
